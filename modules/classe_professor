class Professor:

    #METODO CONSTRUTOR E ATRIBUTOS
    def __init__(self, id, nome, materia):
        try:
            self.set_id(int(id))
            self.set_nome(nome)
            self.set_materia(materia)
        except Exception as e:
            print(str(e))

    #METODOS AUXILIARES
    def get_id(self):
        return self.id

    def set_id(self, id):
        self.id = id

    def get_nome(self):
        return self.nome

    def set_nome(self, nome):
        self.nome = nome

    def get_materia(self):
        return self.materia

    def set_materia(self, materia):
        self.materia = materia

    #METODO "PRINTAR"
    def printar(self):
        try:
            print(f"Id: {self.get_id()}, Nome: {self.get_nome()}, Mat√©ria: {self.get_materia()}")
        except Exception as e:
            print(str(e))
